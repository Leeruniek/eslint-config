{
  "name": "@codemachiner/eslint-config",
  "version": "2.9.6",
  "description": "JavaScript ESLint rules",
  "main": "rules/frontend.js",
  "scripts": {
    "setup": "rm -rf node_modules && npm i && npm audit fix",
    "update": "npm-check -u",
    "lint:js": "eslint ./rules/*",
    "lint:md": "markdownlint *.md",
    "lint": "npm run lint:js && npm run lint:md",
    "prepare": "npm run lint",
    "preversion": "npm run prepare"
  },
  "homepage": "https://github.com/codemachiner/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codemachiner/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/codemachiner/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-rules",
    "react",
    "flow",
    "flowtype",
    "config",
    "javascript",
    "codemachiner"
  ],
  "author": "Andrei Dumitrescu <andrei@codemachiner.com> (https://twitter.com/andreidcm)",
  "dependencies": {},
  "devDependencies": {
    "eslint": "5.5.0",
    "eslint-plugin-compat": "2.5.1",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-html": "4.0.5",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "1.2.1",
    "eslint-plugin-jsx-control-statements": "2.2.1",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-unicorn": "6.0.1",
    "markdownlint-cli": "0.13.0",
    "npm-check": "5.8.0"
  },
  "peerDependencies": {
    "eslint": "^5.5.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-no-inferred-method-name": "^1.0.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-unicorn": "^6.0.1"
  },
  "engines": {
    "node": ">= 6"
  },
  "license": "MIT"
}
